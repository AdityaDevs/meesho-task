{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions"
                    ]
                },
                "modules": false
            }
        ],
        "react",
        "stage-2"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "babel-plugin-transform-decorators-legacy",
        "babel-plugin-dynamic-import-node"
    ],
    "env": {
        "test": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "browsers": [
                                "last 2 versions"
                            ]
                        }
                    }
                ],
                "react",
                "stage-2"
            ]
        }
    }
}